<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="IPFilter.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
    </sectionGroup>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.net>
    <settings>
      <httpWebRequest useUnsafeHeaderParsing="true" />
    </settings>
  </system.net>
  <userSettings>
    <IPFilter.Properties.Settings>
      <setting name="Username" serializeAs="String">
        <value />
      </setting>
      <setting name="Password" serializeAs="String">
        <value />
      </setting>
      <setting name="IsScheduleEnabled" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="ScheduleHours" serializeAs="String">
        <value>6</value>
      </setting>
      <setting name="DisableCache" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="ShowNotifications" serializeAs="String">
        <value>False</value>
      </setting>
    </IPFilter.Properties.Settings>
  </userSettings>
  <system.diagnostics>  
    <sharedListeners>  
      <add name="file" type="IPFilter.Logging.FileTraceListener, IPFilter" />  
    </sharedListeners>
    <trace autoflush="true" indentsize="2" useGlobalLock="false">
      <listeners>
        <add name="file"/>
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>
